# Amazon Bedrock API key (bearer token)
AWS_BEARER_TOKEN_BEDROCK=your-bedrock-api-key-here

# AWS region where Bedrock is enabled
AWS_DEFAULT_REGION=us-east-1

# GitHub personal access token (needs repo scope for creating PRs)
GITHUB_TOKEN=your-github-token-here

# Jira credentials (API token auth)
JIRA_BASE_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your-email@example.com
JIRA_API_TOKEN=your-jira-api-token-here
